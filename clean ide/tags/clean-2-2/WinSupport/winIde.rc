PRODUCTVERSION  2,2,0,0
FILEFLAGSMASK   0x0030003FL
FILEFLAGS       VS_FF_PRERELEASE
FILEOS          VOS__WINDOWS32
FILETYPE        VFT_APP
FILESUBTYPE     VFT2_UNKNOWN
BEGIN
