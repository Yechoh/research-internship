    BLOCK "040904B0"
    BEGIN
      VALUE "CompanyName",        VERSION_RES_COMPANY_NAME
      VALUE "FileDescription",    VERSION_RES_BIN_DESCRIPTION
      VALUE "FileVersion",        VERSION_RES_STRING
