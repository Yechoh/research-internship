      VALUE "LegalCopyright",     VERSION_RES_COPYRIGHT
      VALUE "OriginalFilename",   VERSION_RES_BIN_NAME
      VALUE "ProductName",        VERSION_RES_PRODUCT_NAME
      VALUE "ProductVersion",     VERSION_RES_STRING
    END
  END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
